{
  "mcpServers": {
    "fetch": {
      "args": [
        "mcp-server-fetch"
      ],
      "command": "uvx",
      "disabled": true
    },
    "github": {
      "headers": {
        "Authorization": "Bearer ${GITHUB_PERSONAL_ACCESS_TOKEN}"
      },
      "url": "https://api.githubcopilot.com/mcp/"
    },
    "odoo-ai-quest-mcp": {
      "disabled": true,
      "headers": {
        "Authorization": "Bearer ${ODOO_USER_ACCESS_TOKEN}"
      },
      "url": "http://mcp.azzar.org/api/mcp"
    }
  }
}
